# Profile for import-csv script
timezone = Europe/Amsterdam
identifier = "Cell={cellid}"
identifier_regex = "(.*)"
granularity = 86400
timestamp_format = %Y%m%d_%H%M

# Column containing timestamp for data
# --timestamp option overides this
#timestamp_column = timestamp
timestamp_column =
timestamp_from_filename_regex =

timestamp_is_start = false

identifier_is_alias = false
ignore_field_mismatches = false

# Comma separated list of field names to parse
# Leave empty for parsing all fields
fields =

# Comma separated list of field names to ignore
# Leave empty for ignoring no fields
ignore_fields =

# Json formatted dictionary defining a mapping for specific columns and values
# e.g.
# value_mapping = {"ccr":{"-1":"", "2":"1"}, "cssr":{"-1":"", "2":"1"}}
# (for mapping '-1' to '' (resulting in a Null value in database)
# and '2' to '1' for columns ccr and cssr)
value_mapping =

character_encoding = utf-8
dialect = auto

[aggregate]
# The column used to group the records
group_by = entity_id

# Name of the function to use for aggregation (currently only 'sum' is allowed)
function = sum
